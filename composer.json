{
    "name": "rsporteman/validator",
    "description": "provides a set of commonly needed validators",
    "license": "MIT License",
    "authors": [
        {
            "name": "Raoni Sporteman"
        }
    ],
    "keywords": [
        "rsporteman",
        "validator"
    ],
    "homepage": "https://github.com/rsporteman/validator",
    "autoload": {
        "psr-4": {
            "rsporteman\\Validator\\": "src/"
        }
    },
    "require": {
        "php":"^7.3 || ~8.0.0 || ~8.1.0",
        "laminas/laminas-validator": "^2.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability":"dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "rsporteman\\ValidatorTest\\": "test/src/"
        }
    }
}
